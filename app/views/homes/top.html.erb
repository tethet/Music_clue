<h1>Music_clue topページ</h1>
<% @reviews.each do |review| %>
 <%#= link_to image_tag(review.user.get_user_icon), user_path(review.user.id), class: "rounded-circle" %>
 <%= link_to user_path(review.user.id), class: "rounded-circle" do %> 
    <%= image_tag(review.user.get_user_icon) %>
    <% end %>
 <%= link_to review.user.email, user_path(review.user.id) %>
 <%= review.music_title %>
 <%= review.music_url %>
 <%= review.music_artist_name %>
 <%= review.music_genre %>
 <%= link_to "レビュー詳細へ", review_path(review.id) %>
 <% end %>
 <%= paginate @reviews %>