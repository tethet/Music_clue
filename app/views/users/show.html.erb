<h2><strong>ユーザー詳細</strong></h2>
<%= image_tag @user.user_icon, class: "rounded-circle" %>
<%= @user.email %>
<%= @user.id %>
<%= link_to "いいねした投稿", favorite_user_path(@user) %>
<%= link_to "ユーザー編集", edit_user_path(@user) %>

<% @reviews.each do |review| %>
 <%#= link_to image_tag(@user.user_icon), user_path(@user.id), class: "rounded-circle" %>
 <%= link_to user_path(review.user.id), class: "rounded-circle" do %> 
    <%= image_tag(review.user.get_user_icon) %>
    <% end %>
 <%= link_to @user.email, user_path(@user.id) %>
 <%= review.music_title %>
 <%= review.music_url %>
 <%= review.music_artist_name %>
 <%= review.music_genre %>
 <%= link_to "レビュー詳細へ", review_path(review.id) %>
 <% end %>
 <%= link_to "ユーザーを消去する", ending_user_path(@user) %>
 <%= paginate @reviews %>